{
  "name": "namc-norcal-member-portal",
  "version": "1.0.0",
  "description": "NAMC NorCal Member Portal - Comprehensive digital platform for minority contractors",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:report": "next lint --format json --output-file lint-results.json",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=src/.*\\.test\\.(ts|tsx)$ --testPathIgnorePatterns=integration",
    "test:integration": "jest --testPathPattern=src/.*\\.integration\\.test\\.(ts|tsx)$",
    "test:component": "jest --testPathPattern=src/components/.*\\.test\\.(tsx)$",
    "test:store": "jest --testPathPattern=src/stores/.*\\.test\\.(ts)$",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "e2e:install": "playwright install",
    "clean": "rm -rf .next coverage test-results playwright-report",
    "clean:deps": "rm -rf node_modules package-lock.json && npm install",
    "clean:all": "npm run clean && npm run clean:deps",
    "check-all": "npm run type-check && npm run lint && npm run test:ci",
    "mcp:health": "./scripts/mcp-testing/mcp-health-check.sh",
    "mcp:health:quick": "./scripts/mcp-testing/mcp-health-check.sh --quick",
    "mcp:health:verbose": "./scripts/mcp-testing/mcp-health-check.sh --verbose",
    "mcp:test": "node scripts/mcp-testing/mcp-test-suite.js",
    "mcp:test:verbose": "node scripts/mcp-testing/mcp-test-suite.js --verbose",
    "mcp:test:filesystem": "node scripts/mcp-testing/server-tests/test-filesystem.js",
    "mcp:test:filesystem:verbose": "node scripts/mcp-testing/server-tests/test-filesystem.js --verbose",
    "mcp:monitor": "node scripts/mcp-testing/mcp-monitor.js",
    "mcp:monitor:fast": "node scripts/mcp-testing/mcp-monitor.js --interval 60",
    "mcp:validate-config": "python3 -m json.tool .mcp.json",
    "hubspot:setup": "tsx scripts/hubspot/setup-hubspot-integration.ts",
    "hubspot:sync": "tsx scripts/hubspot/sync-data.ts",
    "hubspot:test": "tsx scripts/hubspot/test-hubspot-connection.ts",
    "hubspot:workflows": "tsx scripts/hubspot/create-workflows.ts",
    "hubspot:properties": "tsx scripts/hubspot/create-properties.ts",
    "hubspot:webhooks": "tsx scripts/hubspot/setup-webhooks.ts",
    "hubspot:health": "tsx scripts/hubspot/health-check.ts",
    "hubspot:monitor": "tsx scripts/hubspot/monitor-workflows.ts --interval 300",
    "hubspot:export": "tsx scripts/hubspot/export-data.ts",
    "hubspot:import": "tsx scripts/hubspot/import-data.ts",
    "hubspot:cleanup": "tsx scripts/hubspot/cleanup-test-data.ts",
    "workflows:validate": "tsx scripts/workflows/validate-workflows.ts",
    "workflows:test": "tsx scripts/workflows/test-workflows.ts",
    "workflows:deploy": "tsx scripts/workflows/deploy-workflows.ts"
  },
  "dependencies": {
    "@hubspot/api-client": "^13.0.0",
    "@prisma/client": "^6.1.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-query": "^5.17.9",
    "@types/bcrypt": "^6.0.0",
    "@types/ioredis": "^4.28.10",
    "@types/react-window": "^1.8.8",
    "axios": "^1.6.5",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.2.0",
    "eventbrite": "^1.3.0",
    "framer-motion": "^10.18.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.312.0",
    "next": "14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-window": "^1.8.11",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "winston": "^3.17.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "husky": "^8.0.3",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^30.0.5",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.1.0",
    "msw": "^2.10.4",
    "postcss": "^8.4.33",
    "prettier": "^3.1.0",
    "prisma": "^6.1.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
