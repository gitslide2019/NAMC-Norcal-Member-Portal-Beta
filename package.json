{
  "name": "namc-norcal-member-portal",
  "version": "1.0.0",
  "description": "NAMC NorCal Member Portal - Comprehensive digital platform for minority contractors",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:integration:watch": "jest --config jest.integration.config.js --watch",
    "test:integration:simple": "jest --config jest.integration.simple.config.js",
    "mcp:health": "./scripts/mcp-testing/mcp-health-check.sh",
    "mcp:health:quick": "./scripts/mcp-testing/mcp-health-check.sh --quick",
    "mcp:health:verbose": "./scripts/mcp-testing/mcp-health-check.sh --verbose",
    "mcp:test": "node scripts/mcp-testing/mcp-test-suite.js",
    "mcp:test:verbose": "node scripts/mcp-testing/mcp-test-suite.js --verbose",
    "mcp:test:filesystem": "node scripts/mcp-testing/server-tests/test-filesystem.js",
    "mcp:test:filesystem:verbose": "node scripts/mcp-testing/server-tests/test-filesystem.js --verbose",
    "mcp:monitor": "node scripts/mcp-testing/mcp-monitor.js",
    "mcp:monitor:fast": "node scripts/mcp-testing/mcp-monitor.js --interval 60",
    "mcp:validate-config": "python3 -m json.tool .mcp.json"
  },
